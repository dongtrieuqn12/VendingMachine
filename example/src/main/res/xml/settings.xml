<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:title="@string/action_settings">
    <com.felhr.serialportexample.Others.EditTextPreferenceWithValue
        android:defaultValue="@string/client_id"
        android:key="client_id"
        android:maxLines="1"
        android:selectAllOnFocus="true"
        android:singleLine="true"
        android:title="@string/client_id_title"
        />
    <com.felhr.serialportexample.Others.EditTextPreferenceWithValue
        android:defaultValue="@string/client_secret"
        android:key="client_secret"
        android:maxLines="1"
        android:numeric="integer"
        android:selectAllOnFocus="true"
        android:singleLine="true"
        android:title="@string/client_secret_title" />
    <com.felhr.serialportexample.Others.EditTextPreferenceWithValue
        android:defaultValue="@string/DIVINP"
        android:key="div_input"
        android:maxLines="1"
        android:selectAllOnFocus="true"
        android:singleLine="true"
        android:title="@string/divinp_title"
        />
    <com.felhr.serialportexample.Others.EditTextPreferenceWithValue
        android:defaultValue="@string/idChinhanh"
        android:key="id_chinhanh"
        android:maxLines="1"
        android:numeric="integer"
        android:selectAllOnFocus="true"
        android:singleLine="true"
        android:title="@string/idchinhanh_title" />
    <com.felhr.serialportexample.Others.EditTextPreferenceWithValue
        android:defaultValue="@string/idsold"
        android:key="id_sold"
        android:maxLines="1"
        android:selectAllOnFocus="true"
        android:singleLine="true"
        android:title="@string/idsold_title"
        />
    <com.felhr.serialportexample.Others.EditTextPreferenceWithValue
        android:defaultValue="@string/terminalId"
        android:key="terminalId"
        android:maxLines="1"
        android:selectAllOnFocus="true"
        android:singleLine="true"
        android:title="terminalId"
        />
    <com.felhr.serialportexample.Others.EditTextPreferenceWithValue
        android:defaultValue="@string/merchantId"
        android:key="merchantId"
        android:maxLines="1"
        android:selectAllOnFocus="true"
        android:singleLine="true"
        android:title="merchantId"
        />

</PreferenceScreen>
